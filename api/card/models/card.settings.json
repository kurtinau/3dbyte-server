{
  "kind": "collectionType",
  "collectionName": "cards",
  "info": {
    "name": "Card"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "type": {
      "type": "string"
    },
    "cardType": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "lastFourDigit": {
      "type": "biginteger"
    },
    "user": {
      "via": "cards",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
